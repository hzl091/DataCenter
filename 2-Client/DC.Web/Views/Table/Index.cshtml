@model IEnumerable<DC.Data.Common.DataManage.TableInfoDto>

@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <a href="Table/Detail?tabName=@item.Name">@item.Desc</a>
            &nbsp;&nbsp;
            <a href="Table/Cols?tabName=@item.Name">[结构]</a>
            &nbsp;&nbsp;
            <a href="Table/AddCol?tabName=@item.Name">[添加列]</a>
            &nbsp;&nbsp;
            <a href="Table/AddData?tabName=@item.Name">[添加数据]</a>
            &nbsp;&nbsp;
        </li>
    }
</ul>
